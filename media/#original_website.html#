    font-size: 18px;
    margin-left: 1px;
    padding-right: 3px;
    -webkit-transition: .4s ease-out;
    color:#76b900
}

.custom-btn a:hover .fa-solid.fa-angle-right{
  color: #000;
    margin-left: 4px;
    padding-right: 0;
}
.custom-btn a {
text-decoration: none;
}
    </style></div>	
    </div>
</div>

    
</div>

        </div>
        
    </div>
    

    
    
    

 

</div>

    
</div>

        </div>
        
    </div>
    

    
    
    

 

