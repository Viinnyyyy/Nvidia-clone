					ubContainer = t_ubContainer;
					var cookieKey = "";
					if(typeof t_cookieKey !== 'undefined' && t_cookieKey.length>0){
						cookieKey = t_cookieKey
					}
					var ubcookie = "";
					if(cookieKey.length>0){
