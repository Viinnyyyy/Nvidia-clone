<script>_satellite["_runScript9"](function(event, target, Promise) {
//Hatch
function getQueryStringValue (key) {  
  return unescape(window.location.search.replace(new RegExp("^(?:.*[&\\?]" + escape(key).replace(/[\.\+\*]/g, "\\$&") + "(?:\\=([^&]*))?)?.*$", "i"), "$1"));  
}   
function getUrlHash(sHash) {
    var sPageURL = location.hash.replace('#', '');
    var sURLHash = sPageURL.split('&');
    for (var i = 0; i < sURLHash.length; i++) {
        var sHashName = sURLHash[i].split('=');
        if (sHashName[0] == sHash) {
            return sHashName[1];
        }
    }
}  

try{
var queryString = window.location.href;
queryString = queryString.toLowerCase();
if (queryString.indexOf('ncid')  > -1 && queryString.indexOf('en-us')  > -1) {
var cid = getUrlHash('ncid');
    if (cid != undefined) {
		_satellite.cookie.set('hatch_cid', cid,30);
    }
  else{
   cid = getQueryStringValue('ncid');
    _satellite.cookie.set('hatch_cid', cid, 30);
  }
}else if (queryString.indexOf('cid')  > -1) {
var cid = getUrlHash('cid');
    if (cid != undefined) {
		_satellite.cookie.set('hatch_cid', cid,30);
    }
  else{
   cid = getQueryStringValue('cid');
    _satellite.cookie.set('hatch_cid', cid, 30);
  }
}  


